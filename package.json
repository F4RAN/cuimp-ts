{
  "name": "cuimp",
  "version": "0.0.1",
  "description": "Node wrapper for curl-impersonate (lexiforest) via CLI",
  "license": "MIT",
  "author": "F4RAN <f4ran@vitalize.dev>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/f4ran/cuimp.git"
  },
  "engines": {
    "node": ">=18.17"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external tar",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "smoke": "node --input-type=module -e \"import('./dist/index.js').then(m=>m.__smoke && m.__smoke())\""
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@types/tar": "^6.1.13",
    "@vitest/ui": "^2.1.8",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "tar": "^7.4.3"
  }
}
