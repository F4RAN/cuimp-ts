{
  "name": "cuimp",
  "version": "0.0.1",
  "description": "Node wrapper for curl-impersonate (lexiforest) via CLI",
  "license": "MIT",
  "author": "F4RAN <f4ran@vitalize.dev>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/f4ran/cuimp.git"
  },
  "engines": { "node": ">=18.17" },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepare": "npm run build",
    "smoke": "node --input-type=module -e \"import('./dist/index.mjs').then(m=>m.__smoke && m.__smoke())\""
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  }
}
